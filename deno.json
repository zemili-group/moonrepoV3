{
  "imports": {
    "@scope/": "./packages/",
    "@oak/oak": "jsr:@oak/oak@^16.1.0",
    "@std/assert": "jsr:@std/assert@1",
    "$fresh/": "https://deno.land/x/fresh@1.6.8/",
    "preact": "https://esm.sh/preact@10.19.6",
    "preact/": "https://esm.sh/preact@10.19.6/",
