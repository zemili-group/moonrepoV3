{
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": true,
  "editor.tabSize": 2,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "css.customData": [".vscode/tailwind.json"],
  "[svelte]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
